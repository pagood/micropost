<h1>All users</h1>
<% flash.each do |message_type,message| %>
<div class="alert alert-<%= message_type %>"><%= message %> </div>
<%end%>
<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%=image_tag gravatar_for user %>
      <%= link_to user.email, user %>
      <%= link_to 'delete',user_path(user),method: :delete,data: {confirm: "you sure?"}%>
    </li>
  <% end %>
</ul>